{
    //"dockerComposeFile": ["../docker-compose.yml","../docker-compose.workspace.yml"],
    "dockerComposeFile": ["../docker-compose.workspace.yml"],
    "service": "workspace",
    "shutdownAction": "stopCompose",
	"workspaceFolder": "/workspaces/dia/devcontainer-workshop/minimal-docker-compose",
	"features": {
        "./features/src/common-utils":{
            "installZsh": "false",
            "userUid": "1000",
            "userGid": "1000",
            "upgradePackages": "false"
        }
    },
    
	// Use 'forwardPorts' to make a list of ports inside the container available locally.
	// "forwardPorts": [],

	// Use 'postCreateCommand' to run commands after the container is created.
	"postCreateCommand": "",

	// Set `remoteUser` to `root` to connect as root instead. More info: https://aka.ms/vscode-remote/containers/non-root.
	"remoteUser": "vscode",
	"overrideCommand": true
}